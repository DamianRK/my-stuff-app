{
  "name": "my-stuff-app",
  "version": "1.0.0",
  "description": "TODOs management application based on react",
  "main": "index.js",
  "author": "Damian Bogdanowicz",
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "npm run styles -- -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DamianRK/my-stuff-app.git"
  },
  "keywords": [
    "react",
    "app",
    "todo",
    "stuff",
    "GTD"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DamianRK/my-stuff-app/issues"
  },
  "homepage": "https://github.com/DamianRK/my-stuff-app#readme",
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "devDependencies": {
    "concurrently": "4.1.0",
    "react-scripts": "^2.1.1"
  }
}
